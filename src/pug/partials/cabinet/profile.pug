include ../profile-photo.pug
include ../profile-action.pug
include ../profile-action-msg.pug

aside.cabinet__profile-container.scroll-back__panel
  +profile-photo({
    linkText: "Измените фото",
    linkClass: "profile-photo__link--cabinet"
  })
  nav.cabinet__actions
    +profile-action("#help", "profile-action--dark", "", "Your profile")
    +profile-action-msg("./messages.html", "Сообщения")
    +profile-action("./settings.html", "profile-action--dark", "", "Настройки")
